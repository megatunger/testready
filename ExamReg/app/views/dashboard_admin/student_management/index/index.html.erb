<%= render partial: 'dashboard_admin/student_management/index/include' %>
<div class="wrapper">
  <div class="container-fluid">
    <!-- Page-Title -->
    <div class="page-title-box">
      <div class="row align-items-center">
        <div class="col-sm-6">
          <h4 class="page-title">Quản lí học sinh</h4>
        </div>
        <div class="col-sm-6">
          <ol class="breadcrumb float-right">
            <li class="breadcrumb-item"><a href=<%=root_path%>>TestReady</a></li>
            <li class="breadcrumb-item active">Quản lí học sinh</li>
          </ol>
        </div>
      </div>
      <!-- end row -->
    </div>

    <div class = "pb-4">
      <%= render partial: 'dashboard_admin/student_management/index/toolbar' %>
    </div>



    <div class="row">
      <div class="col-12">
        <div class="card m-b-30">
          <div class="card-body">
            <div class="container row align-items-center mb-4">
              <h4 class="ml-3 mt-0 mr-3 header-title">Danh sách học sinh</h4>
              <div id="loading" class="spinner-border text-primary" role="status" style = "display: none">
                <span class="sr-only">Loading...</span>
              </div>
            </div>
            <table id="datatable-buttons" class="table table-striped table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
              <thead>
              <tr>
                <th> </th>
                <th data-priority="3">MSSV</th>
                <th data-priority="1">Họ</th>
                <th data-priority="3">Tên</th>
                <th data-priority="3">Ngày sinh</th>
                <th data-priority="6">Giới tính</th>
                <th data-priority="6">Lớp</th>
                <th data-priority="6">Ngành</th>
                <th data-priority="6">Khoa</th>
                <th data-priority="9">Cập nhật lúc</th>
              </tr>
              </thead>
            </table>

          </div>
        </div>
      </div> <!-- end col -->
    </div> <!-- end row -->
  </div>
  <!-- end container-fluid -->
</div>
<!-- end wrapper -->