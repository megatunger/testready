<div class="modal-header">
  <h5 class="modal-title mt-0" id="myLargeModalLabel">Danh sách sinh viên đăng kí</h5>
  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>

<div class="modal-body">
  <div class="card m-b-30" style = "background: none">
    <%= render partial: 'admin/schedule_management/show/paper' %>
  </div>
</div>

<div class="modal-footer">
  <div class="d-print-none mo-mt-2">
    <div class="float-right">
      <%= link_to "<i class='fa fa-print'></i> Tạo PDF & In".html_safe, admin_exam_schedule_export_pdf_path(@exam, @exam_schedule, :format => :pdf), class: 'btn btn-success waves-effect waves-light', target: :_blank %>
      <button type="button" class="btn btn-secondary" data-dismiss="modal">Đóng</button>
    </div>

  </div>
</div>
